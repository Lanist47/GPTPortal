{
  "version": 2,
  "routes": [
    {
      "src": "/portal",
      "dest": "/public/portal.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "",
    "USER_USERNAME": "",
    "USER_PASSWORD": "",
    "GOOGLE_API_KEY": "",
    "MISTRAL_API_KEY": "",
    "CLAUDE_API_KEY": ""
  },
  "build": {
    "env": {}
  }
}
